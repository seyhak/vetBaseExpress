{
  "name": "VetBase",
  "version": "1.0.0",
  "author": "Seyhak <seyhakly@gmail.com>",
  "description": "App for knowledge base",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@google-cloud/local-auth": "2.1.0",
    "@mui/icons-material": "^5.14.14",
    "@mui/material": "^5.14.14",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "classnames": "^2.3.2",
    "googleapis": "105",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0",
    "react-scripts": "5.0.1",
    "sass": "^1.69.4",
    "slate": "^0.100.0",
    "slate-history": "^0.100.0",
    "slate-react": "^0.100.0",
    "sqlite3": "^5.1.6",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "yarn prettier --write ."
    ]
  },
  "imports": {
    "#components/*": "./src/components/*"
  },
  "scripts": {
    "start": "set REACT_APP_DEV=1 & react-scripts start",
    "watch": "tsc -w",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "rules": {
      "quotes": [
        "warn",
        "double"
      ],
      "semi": [
        "warn",
        "never"
      ],
      "indent": [
        "warn",
        2
      ]
    },
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3"
  }
}
